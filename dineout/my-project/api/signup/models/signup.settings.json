{
  "kind": "collectionType",
  "collectionName": "signups",
  "info": {
    "name": "Signup"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "password": {
      "type": "string",
      "private": true,
      "required": true
    },
    "firstName": {
      "type": "string",
      "required": true
    },
    "secondName": {
      "type": "string"
    },
    "email": {
      "type": "email",
      "required": true
    }
  }
}
